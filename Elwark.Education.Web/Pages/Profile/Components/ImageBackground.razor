@* ReSharper disable once CheckNamespace *@
@namespace Elwark.Education.Web.Components

<div class="background @Class">
    <div class="cover">
        @ChildContent
    </div>
</div>

@code {

    [Parameter]
    public string? Class { get; set; }

    [Parameter]
    public RenderFragment ChildContent { get; set; } = default!;

}