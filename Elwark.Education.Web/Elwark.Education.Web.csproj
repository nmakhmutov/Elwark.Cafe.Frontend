<Project Sdk="Microsoft.NET.Sdk.BlazorWebAssembly">

    <PropertyGroup>
        <TargetFramework>net5.0</TargetFramework>
        <Nullable>enable</Nullable>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="5.0.0" />
        <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.DevServer" Version="5.0.0" PrivateAssets="all" />
        <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.Authentication" Version="5.0.0" />
        <PackageReference Include="Microsoft.AspNetCore.Components.Web.Extensions" Version="5.0.0-preview9.20513.1" />
        <PackageReference Include="Microsoft.Extensions.Http" Version="5.0.0" />
        <PackageReference Include="MudBlazor" Version="1.1.5" />
        <PackageReference Include="Markdig" Version="0.22.0" />
        <PackageReference Include="Blazored.FluentValidation" Version="1.3.2" />
        <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
        <PackageReference Include="Microsoft.Extensions.Localization" Version="5.0.0" />
        <PackageReference Include="Blazored.LocalStorage" Version="3.0.0" />
    </ItemGroup>

    <ItemGroup>
      <EmbeddedResource Update="Resources\Pages.History.Test.resx">
        <Generator>ResXFileCodeGenerator</Generator>
        <LastGenOutput>Pages.History.Test.Designer.cs</LastGenOutput>
      </EmbeddedResource>
      <EmbeddedResource Update="Resources\Pages.History.Test.ru.resx">
        <Generator>ResXFileCodeGenerator</Generator>
        <LastGenOutput>Pages.History.Test.Designer.cs</LastGenOutput>
      </EmbeddedResource>
      <EmbeddedResource Update="Resources\Shared.ProfileLayout.resx">
        <Generator>ResXFileCodeGenerator</Generator>
        <LastGenOutput>Shared.ProfileLayout.Designer.cs</LastGenOutput>
      </EmbeddedResource>
      <EmbeddedResource Update="Resources\Shared.ProfileLayout.ru.resx">
        <Generator>ResXFileCodeGenerator</Generator>
        <LastGenOutput>Shared.ProfileLayout.Designer.cs</LastGenOutput>
      </EmbeddedResource>
    </ItemGroup>

    <ItemGroup>
      <Compile Update="Resources\Pages.History.Test.Designer.cs">
        <AutoGen>True</AutoGen>
        <DependentUpon>Pages.History.Test.resx</DependentUpon>
        <DesignTime>True</DesignTime>
      </Compile>
    </ItemGroup>

</Project>
